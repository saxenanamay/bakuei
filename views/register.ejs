<% include partials/header %>
<canvas id="myCanvas" width="200" height="200"></canvas>
<div class="container text-center signup-form-container">
<form action="/register" method="POST">
    <h1>Sign Up</h1>
    <input class="form-control login-field" type="text" name="username" placeholder="username" required>
    <input class="form-control login-field" type="password" name="password" placeholder="password" required>
    <input class="form-control login-field" type="text" name="adminCode" placeholder="cpde">
    <input class="form-control login-field" type="text" name="state" placeholder="state" required>
    <input class="form-control login-field" type="text" name="country" placeholder="country" required>
    <button class="form-login-button">Sign Up</button>
</form>
</div>

<% include partials/footer %>
